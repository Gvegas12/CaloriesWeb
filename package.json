{
	"name": "bi-vite",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "cross-env ENV_FILE=development vite",
		"build:prod": "tsc && cross-env ENV_FILE=production vite build",
		"build:stage": "tsc && cross-env ENV_FILE=stage vite build",
		"build:dev": "tsc && cross-env ENV_FILE=development vite build",
		"lint:ts": "eslint \"**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0 --fix **/*.{ts,tsx}",
		"preview": "cross-env ENV_FILE=development vite preview",
		"create:page": "gvegas-cfc create -t ./lib/templates/AsyncTemplate -o ./src/pages",
		"create:ui": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/shared/UI/",
		"create:widget": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/widgets/",
		"create:feature": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/features/",
		"prepare": "husky install"
	},
	"dependencies": {
		"@ant-design/icons": "^5.3.7",
		"@hookform/resolvers": "^3.3.4",
		"@neodrag/react": "^2.0.3",
		"antd": "^5.18.1",
		"axios": "^1.6.5",
		"clsx": "^2.1.0",
		"dayjs": "^1.11.10",
		"immer": "^10.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.51.1",
		"react-router-dom": "^6.21.2",
		"react-select": "^5.8.0",
		"react-transition-group": "^4.4.5",
		"usehooks-ts": "^2.10.0",
		"zod": "^3.22.4",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^5.1.0",
		"@types/node": "^20.11.5",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.19.0",
		"@vitejs/plugin-react": "^4.2.1",
		"cross-env": "^7.0.3",
		"dotenv": "^16.3.1",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"eslint-plugin-storybook": "^0.6.14",
		"gvegas-cfc": "^3.0.9",
		"husky": "^8.0.3",
		"prettier": "^3.2.4",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.70.0",
		"typescript": "^5.2.2",
		"uuid": "^9.0.1",
		"vite": "^5.0.8",
		"vite-plugin-static-copy": "^1.0.0",
		"vite-plugin-svgr": "^4.2.0"
	}
}
